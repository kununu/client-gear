  },
  "author": "kununu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kununu/client-gear/issues"
  },
  "homepage": "https://github.com/kununu/client-gear#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@karlhorky/react-scripts": "0.8.3-alpha.2",
    "@kununu/eslint-config": "1.0.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-rename-import": "2.3.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.11.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-localstorage-mock": "2.3.0",
    "lerna": "2.11.0",
    "node-sass": "4.11.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-test-renderer": "16.7.0",
    "sass-loader": "7.1.0",
    "sasslint-loader": "0.0.1",
    "webpack": "3.3.0"
  },
  "scripts": {
    "lint": "eslint packages --max-warnings 0 --ext jsx --ext js",
    "test": "jest --verbose --no-cache",
    "test:watch": "jest --watch --verbose",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@kununu/kununu-theme-v2": "2.0.2"
