{
  "name": "@kununu/kununu-footer",
  "version": "2.4.2",
  "description": "kununu Footer",
  "main": "dist",
  "author": "kununu",
  "license": "ISC",
  "devDependencies": {
    "@kununu/kununu-theme-v2": "2.0.2",
    "node-sass": "4.11.0",
    "node-sass-module-importer": "0.1.0",
    "nukleus": "14.1.8"
  },
  "dependencies": {
    "@kununu/kununu-icons": "1.4.0",
    "@kununu/kununu-logo": "1.1.0",
    "@kununu/kununu-theme-v2": "2.0.2"
  },
  "peerDependencies": {
    "nukleus": "14.1.8",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "prepare": "npm run dist",
    "babel": "babel $(find ./ -name '*.jsx' ! -name '*.test.jsx' ! -ipath '*node_modules*') --out-dir ./dist",
    "sass": "node-sass ./index.scss --output ./dist --importer node_modules/node-sass-module-importer",
    "clean": "rimraf ./dist",
    "dist": "npm run clean && cross-env NODE_ENV=production npm run babel && npm run sass"
  }
}
