{
  "name": "@kununu/kununu-footer",
  "version": "3.1.0-beta.1",
  "description": "kununu Footer",
  "main": "dist",
  "author": "kununu",
  "license": "ISC",
  "dependencies": {
    "@kununu/kununu-icons": "1.6.1",
    "@kununu/kununu-logo": "1.2.2",
    "@kununu/kununu-theme-v2": "2.0.2",
    "@kununu/kununu-utils": "1.27.0-beta.0",
    "node-sass-module-importer": "0.1.0",
    "nukleus": "14.1.12"
  },
  "peerDependencies": {
    "@kununu/kununu-icons": "1.6.1",
    "@kununu/kununu-logo": "1.2.2",
    "nukleus": "14.1.12",
    "prop-types": ">=15.6.0",
    "react": ">=16.0.0"
  },
  "scripts": {
    "prepare": "npm run dist",
    "babel": "babel src --out-dir ./dist --root-mode upward",
    "sass": "node-sass ./src/index.scss --output ./dist --importer node_modules/node-sass-module-importer",
    "clean": "rimraf ./dist",
    "dist": "npm run clean && cross-env NODE_ENV=production npm run babel && npm run sass"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "rimraf": "3.0.2",
    "@babel/cli": "7.2.3",
    "node-sass": "4.11.0",
    "node-sass-module-importer": "0.1.0"
  }
}
