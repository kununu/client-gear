{
  "name": "@kununu/kununu-footer",
  "version": "1.2.0",
  "description": "kununu Footer",
  "main": "dist",
  "author": "kununu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "cross-env": "5.2.0",
    "kununu-theme": "3.1.3",
    "node-sass": "4.9.0 ",
    "node-sass-module-importer": "0.1.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "rimraf": "2.6.2",
    "nukleus": "12.0.2"
  },
  "dependencies": {
    "@kununu/kununu-theme-v2": "2.0.1",
    "kununu-logo": "3.3.0",
    "prop-types": "15.6.2"
  },
  "peerDependencies": {
    "@kununu/kununu-theme-v2": "2.0.1",
    "nukleus": "12.0.2",
    "react": "16.4.1",
    "react-dom": "16.4.1"
  },
  "scripts": {
    "prepublish": "yarn dist",
    "babel": "babel $(find ./ -name '*.jsx' ! -name '*.test.jsx' ! -ipath '*node_modules*') --out-dir ./dist",
    "sass": "node-sass ./index.scss --output ./dist --importer node_modules/node-sass-module-importer",
    "clean": "rimraf ./dist",
    "dist": "npm run clean && cross-env NODE_ENV=production yarn babel && yarn sass"
  }
}
