{
  "name": "@kununu/kununu-footer",
  "version": "2.4.0",
  "description": "kununu Footer",
  "main": "dist",
  "author": "kununu",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@kununu/kununu-theme-v2": "2.0.2",
    "cross-env": "5.2.0",
    "node-sass": "4.11.0 ",
    "node-sass-module-importer": "0.1.0",
    "nukleus": "13.1.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "@kununu/kununu-theme-v2": "2.0.2",
    "@kununu/kununu-icons": "1.2.0",
    "@kununu/kununu-logo": "1.1.0",
    "prop-types": "15.6.2"
  },
  "peerDependencies": {
    "nukleus": "13.1.1",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "prepublish": "yarn dist",
    "babel": "babel $(find ./ -name '*.jsx' ! -name '*.test.jsx' ! -ipath '*node_modules*') --out-dir ./dist",
    "sass": "node-sass ./index.scss --output ./dist --importer node_modules/node-sass-module-importer",
    "clean": "rimraf ./dist",
    "dist": "npm run clean && cross-env NODE_ENV=production yarn babel && yarn sass"
  }
}
