{
  "name": "@kununu/kununu-overlay",
  "version": "1.1.0",
  "description": "kununu page loading spinner",
  "main": "dist",
  "author": "kununu",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@kununu/kununu-theme-v2": "2.0.2",
    "cross-env": "5.2.0",
    "node-sass": "4.11.0",
    "node-sass-module-importer": "0.1.0",
    "react": "16.8.1",
    "rimraf": "2.6.3"
  },
  "peerDependencies": {
    "@kununu/kununu-theme-v2": "2.0.2",
    "react": "16.8.1",
    "prop-types": "15.7.1"
  },
  "scripts": {
    "prepare": "yarn dist",
    "babel": "babel ./index.jsx --out-dir ./dist",
    "sass": "node-sass ./index.scss --output ./dist --importer node_modules/node-sass-module-importer",
    "clean": "rimraf ./dist",
    "dist": "npm run clean && cross-env NODE_ENV=production yarn babel && yarn sass"
  }
}
